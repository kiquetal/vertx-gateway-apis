{
  "timestamp": {
    "$resolver": "timestamp",
    "pattern": "yyyy-MM-dd HH:mm:ss.SSS"
  },
  "level": {
    "$resolver": "level",
    "field": "name"
  },
  "thread": {
    "$resolver": "thread",
    "field": "name"
  },
  "logger": {
    "$resolver": "logger",
    "field": "name"
  },
  "message": {
    "$resolver": "message",
    "stringified": true
  },
  "exception": {
    "$resolver": "exception",
    "field": "message"
  }
}
