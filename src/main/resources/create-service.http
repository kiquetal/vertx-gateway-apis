  "burstCapacity": 100,
  "rateLimitPerSecond": 10,
  "endpoints": [
    {
      "name": "sayHello",
      "methodName": "SayHello",
      "inputMessage": "HelloRequest",
      "outputMessage": "HelloReply",
      "inputMapping": {},
      "outputMapping": {}
    }
  ]
}
